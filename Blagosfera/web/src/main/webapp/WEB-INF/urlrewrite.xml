<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 4.0//EN" "http://www.tuckey.org/res/dtds/urlrewrite4.0.dtd">
<urlrewrite>
    <rule>
        <name>Subgroups</name>
        <note>
            Replace all /sg/ to %252Fsg%252F
        </note>
        <from>/sg/</from>
        <to>%252Fsg%252F</to>
    </rule>
</urlrewrite>
